drop table invoice if exists;
drop table invoice_line_item if exists;
drop table task if exists;
create table invoice (id bigint generated by default as identity (start with 1), client_email varchar(255), client_name varchar(255), client_num varchar(255), client_status varchar(255), client_email_sent_dt date, invoice_loc varchar(255), invoice_num varchar(255), client_filename varchar(255), task_id bigint not null, primary key (id));
create table invoice_line_item (id bigint generated by default as identity (start with 1), balance double, currency varchar(255), doc_dt date, doc_id varchar(255), range1 double, range2 double, range3 double, range4 double, invoice_id bigint not null, primary key (id));
create table task (id bigint generated by default as identity (start with 1), complete_dt date, completed boolean, create_dt date, filename varchar(255), primary key (id));
alter table invoice add constraint FKc4aoco04bdp7dq562l4cmt58c foreign key (task_id) references task;
alter table invoice_line_item add constraint FK4560gqjs1jn62cp4govj4akb3 foreign key (invoice_id) references invoice;